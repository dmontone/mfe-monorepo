{
  "name": "mfe-whitelabel",
  "private": true,
  "scripts": {
    "reset": "npx rimraf node_modules",
    "clean": "npx rimraf .next .turbo node_modules/.cache apps/*/.next apps/*/.turbo apps/*/node_modules/.cache",
    "dev": "npm run clean && turbo run dev",
    "dev:clean": "npm run clean && turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.8.11",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.6.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
